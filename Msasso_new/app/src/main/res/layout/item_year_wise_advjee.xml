<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardCornerRadius="4dp"
    android:padding="12dp"
    android:orientation="vertical"
    android:layout_margin="8dp">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_year"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textFontWeight="700"
            android:layout_weight="1"
            android:fontFamily="@font/lato"
            android:textColor="#000000"
            android:text="2019" />


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ll_paper1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="15dp"
                android:layout_marginEnd="8dp"
                android:layout_weight="1"
                android:background="@drawable/bg_grad_tab_select"
                android:orientation="vertical">



                <TextView
                    android:id="@+id/tv_paper1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="18sp"
                    android:fontFamily="@font/lato"
                    android:textColor="#ffffff"
                    android:letterSpacing="0.04"
                    android:text="Paper-1" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/ll_accuracy_p1"
                        android:layout_marginTop="15dp"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <TextView
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Accuracy"
                            android:textSize="12sp"
                            android:fontFamily="@font/lato"
                            android:textColor="#64ffffff"/>
                        <TextView
                            android:id="@+id/tv_accuracy_p1"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="0.00"
                            android:textFontWeight="700"
                            android:textSize="18sp"
                            android:fontFamily="@font/lato"
                            android:textColor="#ffffff"/>


                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="15dp"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/tv_p1_att"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Attempts"
                            android:textSize="12sp"
                            android:fontFamily="@font/lato"
                            android:textColor="#64ffffff"/>
                        <TextView
                            android:id="@+id/tv_noof_attempts_p1"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textFontWeight="700"
                            android:textSize="18sp"
                            android:fontFamily="@font/lato"
                            android:textColor="#ffffff"/>


                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_paper2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="15dp"
                android:layout_marginStart="8dp"
                android:layout_weight="1"
                android:background="@drawable/bg_grad_tab_select"
                android:orientation="vertical">



                <TextView
                    android:id="@+id/tv_paper2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="18sp"
                    android:fontFamily="@font/lato"
                    android:textColor="#ffffff"
                    android:letterSpacing="0.04"
                    android:text="Paper-2" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/ll_accuracy_p2"
                        android:layout_marginTop="15dp"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <TextView
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Accuracy"
                            android:textSize="12sp"
                            android:fontFamily="@font/lato"
                            android:textColor="#64ffffff"/>
                        <TextView
                            android:id="@+id/tv_accuracy_p2"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="0.00"
                            android:textFontWeight="700"
                            android:textSize="18sp"
                            android:fontFamily="@font/lato"
                            android:textColor="#ffffff"/>


                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="15dp"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/tv_p2_att"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Attempts"
                            android:textSize="12sp"
                            android:fontFamily="@font/lato"
                            android:textColor="#64ffffff"/>
                        <TextView
                            android:id="@+id/tv_noof_attempts_p2"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textFontWeight="700"
                            android:textSize="18sp"
                            android:fontFamily="@font/lato"
                            android:textColor="#ffffff"/>


                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</LinearLayout>

